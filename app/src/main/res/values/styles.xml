<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryUWColor</item>
        <item name="colorPrimaryDark">@color/primaryUWDarkColor</item>
        <item name="colorAccent">@color/secondaryUWColor</item>
        <item name="android:statusBarColor">@color/primaryUWLightColor</item>
        <item name="android:windowBackground">@color/primaryColor</item>
    </style>

    <style name="AppTheme_Alt" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/secondaryColor</item>
        <item name="android:statusBarColor">@color/secondaryLightColor</item>
        <item name="android:background">@color/secondaryDarkColor</item>
    </style>
</resources>